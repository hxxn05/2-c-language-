#include <stdio.h>

int main()
{
	int x, y, z,result;

	printf("세 정수를 입력하시오: ");
	scanf_s("%d %d %d", &x, &y, &z);

	result = (x + y) * (x + z) / (y % z);

	printf("결과값 : %d\n", result);

	return 0;

}

#include <stdio.h>
int main()
{
	int x, y, result;
	printf("두 수를 입력하시오: ");
	scanf_s("%d%d", &x, &y);

	result = (x > y) ? y : x;
	printf("결과값: %d", result);

	return 0;

}


#include <stdio.h>
int main()
{
	int a, b, c, result;
	printf("세 수를 입력하시오: ");
	scanf_s("%d%d%d", &a, &b, &c);

	result = (a < b) ? (b > c) ? b : c : (a > c) ? a : c;

	printf("결과값: %d", result);

	return 0;
}

#include <stdio.h>
int main()
{
	int won, dollar;
	printf("값을 입력하시오: ");
	scanf_s("%d", &dollar);

	won = dollar * 1448.70;

	printf("환율 가격: %d", won); 

	return 0;
}

#include <stdio.h>
int main()
{
	int h, m, s,result;
	printf("현재 시간,분,초를 입력하시오: ");
	scanf_s("%d%d%d", &h, &m, &s);

	h = 3600 * h;
	m = 60 * m;
	s = 60 * s;

	result = h + m + s;
	printf("지난 초: %d", result);

	
}
